{
    "name": "label-tool",
    "displayName": "label-tool",
    "description": "COMMON_LABEL新增选择",
    "keywords": [
        "vscode",
        "plugin",
        "demo"
    ],
    "version": "0.0.1",
    "publisher": "zhoulongli",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "icon": "images/icon.png",
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.resetInitialData",
                "title": "重置Label数据源"
            },
            {
                "command": "extension.enableLabelTool",
                "title": "启用Label Tool"
            }
        ],
        "keybindings": [
            {
                "command": "extension.resetInitialData",
                "key": "alt+f5",
                "mac": "cmd+f5",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "repository": {
        "type": "git",
        "url": "https://github.com/lijf0412/label-tool"
    },
    "homepage": "https://github.com/lijf0412/label-tool/blob/main/README.md",
    "dependencies": {
        "@babel/parser": "^7.13.15"
    }
}
